{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api/*.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}